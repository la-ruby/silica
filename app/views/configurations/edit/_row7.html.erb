<tr>
  <% form_id = random_id %>
  <%= form_with(url: configuration_path, method: :patch, id: form_id) do |form| %>
    <td>
      Marketplace::StrongestStyle<br>
      <span classs="fst-italic">The most eye catching peice of text in the page</span>
    </td>
    <td>
      <%= configuration_size_tag(:marketplace_strongest_style_size, Setting.marketplace_strongest_style_size, form_id) %>
    </td>
    <td>
      <%= configuration_weight_tag(:marketplace_strongest_style_weight, Setting.marketplace_strongest_style_weight, form_id) %>
    </td>
    <td>
      <%= configuration_face_tag(:marketplace_strongest_style_face, Setting.marketplace_strongest_style_face, form_id) %>
    </td>
    <td>
      <%= configuration_color_tag(:marketplace_strongest_style_color, 'Color', Setting.marketplace_strongest_style_color, form_id)  %>
    </td>
  <% end %>
</tr>
