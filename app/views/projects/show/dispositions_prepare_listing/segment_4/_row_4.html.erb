<div class="row ms-2">

  <div class="col-2 p-2">
    <div class="mb-2">
      <%= silica_label('Year') %>
      <%= text_field_tag 'dispositions_prepare_listing_pane[year]', project.listing.year.presence || project.year, class: ['form-control', *inherit_style], form: form_id, data: { action: 'dispositions-prepare-listing-pane#disableSendMarketingEmailButton' } %>
    </div>
  </div>

  <div class="col-2 p-2">
    <div class="mb-2">
      <%= silica_label('Lot Size') %>
      <%= text_field_tag 'dispositions_prepare_listing_pane[lot_size]', project.listing.lot_size.presence || project.lot_size, class: ['form-control', *inherit_style], form: form_id, data: { action: 'dispositions-prepare-listing-pane#disableSendMarketingEmailButton' } %>
    </div>
  </div>

  <div class="col-2 p-2">
    &nbsp;
  </div>

</div>
